        <center><h1>Domena</h1>

	<% if(name == "Domena już istnieje") { %>
         <div class="form-group has-warning has-feedback">
                <label for="inputWarning2">Ostrzeżenie!</label>
                <input type="text" value="<%= name  %>" style="text-align:center;"  class="form-control" id="inputWarning2">
                <span class="glyphicon glyphicon-warning-sign form-control-feedback"></span>
        </div>
	 <% } else { %>
        <div class="form-group has-success has-feedback">
                <label for="inputSuccess2">Operacja wykonana pomyślnie!</label>
                <input type="text" name="name" <center>value="<%= name  %>" style="text-align:center;" class="form-control" id="inputSuccess2">
                <span class="glyphicon glyphicon-ok form-control-feedback"></span>
        </div>

	 <% } %>

