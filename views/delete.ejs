    <center><h1>Usuwanie plików z S3</h1>	
    <div class="container">

        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                <form action="/delete" method="GET">
                    <table class="table table-hover">
                      <tr>
                        <th>Wybierz</th>
                        <th>Nazwa</th>
                      </tr>
                    <% elements.forEach(function(elem){ %>
                        <tr>
                            <td><input type="checkbox" value="<%= elem.Key  %>" name="keys"/></td>
                            <td><%= elem.Key.replace(prefix+"/", "")  %></td>
                        </tr>
                    <%}); %> 
                    </table>
                    <center><input type="submit" name="usun" class=btn btn-danger" value="usuń">
                </form>
            </div>
        </div>

    </div>


